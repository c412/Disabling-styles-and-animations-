{    "manifest_version": 3,    "name": "S-OFF",    "version": "1.0",    "permissions": [ "storage", "activeTab", "scripting" ],    "action": {        "default_popup": "popup.html"    },    "icons": {        "16": "icons/icon16.png",        "48": "icons/icon48.png",        "128": "icons/icon128.png"    },    "content_scripts": [        {            "matches": [ "<all_urls>" ],            "js": [ "content.js" ]        }    ]}